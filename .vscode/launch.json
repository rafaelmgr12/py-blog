{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "run",
                "--host=127.0.0.1",
                "--port=8000",
                "--reload"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1"
            },
            "jinja": true
        }
    ]
}
